// assign() 멤버 함수 검증 함수
void test_assign()
{
    cout << "===== vector::assign() 멤버 함수 검증 =====\n";

    vector<int> v;

    // assign(n, value) : 같은 값을 여러 개 채우기
    v.assign(5, 10);    // 요소 10을 5개 넣기
    print_vector(v, "1) assign(5, 10) 호출 후");

    // 2) assign(first, last) : 다른 vector 구간 복사
    vector<int> src = { 1, 2, 3, 4 };
    v.assign(src.begin(), src.end());
    print_vector(v, "2) assign(src.begin(), src.end()) 호출 후");

    // 3) assign({initializer_list}) : 초기화 리스트 사용
    v.assign({ 7, 8, 9 });
    print_vector(v, "3) assign({7,8,9}) 호출 후");
}

int main()
{
    test_assign();
    return 0;
}*/
