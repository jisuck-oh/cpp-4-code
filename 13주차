/*class rect {
    int height;
    int width;
public:
    rect() : height(0), width(0){} // 기본 생성자
    rect(int h, int w) : height(h), width(w){} // 매개변수있는 생성자

    int area() const { // 넓이 계산 함수
        return height * width;
    }

    void print() const { // 사각형 정보 출력 함수
        cout << "height:" << height
            << ", width:" << width
            << ", area:" << area() << endl;
    }
};

int main() {
    vector<rect> rects; // rect 객체를 저장할 vector

    // 사각형 3개 생성해서 push_back으로 값 저장
    rects.push_back(rect(3, 5));
    rects.push_back(rect(10, 2));
    rects.push_back(rect(7, 7));

    for (auto& r : rects) { // vector에 저장된 사각형 정보 출력
        r.print();
    }

    int totalArea = 0; // 사각형 전체 넓이 합
    for (auto& r : rects) {
        totalArea += r.area();
    }

    cout << "전체 사각형 넓이 합:" << totalArea << endl;
}*/



